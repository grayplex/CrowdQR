<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat code coverage">
				<Configuration>
					<Format>cobertura</Format>
					<Format>opencover</Format>
					<Exclude>[*.Tests]*,[*]*.Program,[*]*.Startup</Exclude>
					<ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
					<ExcludeByFile>**/Migrations/**/*.cs,**/wwwroot/**/*.cs</ExcludeByFile>
					<SingleHit>false</SingleHit>
					<UseSourceLink>true</UseSourceLink>
					<IncludeTestAssembly>false</IncludeTestAssembly>
					<SkipAutoProps>true</SkipAutoProps>
					<DeterministicReport>true</DeterministicReport>
					<ThresholdForCyclomaticComplexity>20</ThresholdForCyclomaticComplexity>
					<ThresholdForNPathComplexity>200</ThresholdForNPathComplexity>
					<ThresholdForCrapScore>30</ThresholdForCrapScore>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>