{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Warning",
      "CrowdQR.Api.Hubs": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "CrowdQR.Api",
    "Audience": "CrowdQR.Web"
  },
  "AppSettings": {
    "BaseUrl": "${APP_BASE_URL}"
  }
}
